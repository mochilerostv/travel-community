{
  "version": 2,
  "name": "travel-community",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "AI_PROXY_TOKEN": "@ai_proxy_token"
  },
  "functions": {
    "app/api/wp/ai-extract/route.ts": {
      "maxDuration": 30
    },
    "app/api/ingest/route.ts": {
      "maxDuration": 60
    }
  }
}
